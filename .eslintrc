{
  "root": true,
  "env": {
    "browser": true
  },
  "extends": ["plugin:prettier/recommended", "prettier/@typescript-eslint"],
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [".ts", ".tsx"]
    }
  }
}
